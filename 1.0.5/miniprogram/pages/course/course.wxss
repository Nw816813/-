.boxing{
  width: 100vw;
  padding-top: 50rpx;
}

.boxing>.item:last-child{
  padding-bottom: 0 !important;
  border: none !important;
}

.item{
  width: 90%;
  margin: 20rpx auto;
  color: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  border-bottom: 2rpx solid #e2e2e2;
  padding-bottom: 20rpx;
}

.avatar{
  width: 100rpx;
  height: 100rpx;
}

.input{
  border: 1rpx solid #fff;
  border-radius: 10rpx;
  padding: 10rpx;
}

.sp{
  width: 250rpx;
}

.labelsp{
  margin-bottom: 20rpx;
}

.over{
  overflow: scroll;
}

.itemsp{
  align-items: flex-start;
  flex-direction: column;
  overflow: scroll;

}

.picker{
  border: 1rpx solid #fff;
  border-radius: 10rpx;
  padding: 10rpx;
  width: 150rpx;
  height: 40rpx;
}

.itemBtn{
  margin-top: 100rpx;
}

.login{
  width: 100%;
  height: 65rpx !important;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 20rpx;
  background: linear-gradient(147.29deg, #86F0B1 -1.9%, #88C8EF 28.24%, #A0B1FD 62.69%, #F0B2E7 95.36%);
  box-shadow: 0px 50px 100px -20px rgba(50, 50, 93, 0.25), 0px 30px 60px -30px rgba(0, 0, 0, 0.3), inset 0px -2px 6px rgba(10, 37, 64, 0.35);
  color: #fff;
}