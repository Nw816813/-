.box3{
  width: 330px;
  height: 64px;
  display: flex;
  flex-direction: row;
  margin-left: 24px;
  background-color: #262A34;
  border-radius: 16px;
  box-shadow: 0px 5px 15px rgba(0, 0, 0, 0.35);
  align-items: center;
  margin-top: 20px;
